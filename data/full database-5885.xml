<drug type="small molecule" created="2010-09-14" updated="2019-12-02">
  <drugbank-id primary="true">DB06768</drugbank-id>
  <name>Ammonium lactate</name>
  <description>Ammonium lactate is the ammonium salt of lactic acid.</description>
  <cas-number>515-98-0</cas-number>
  <unii>67M901L9NQ</unii>
  <average-mass>107.1085</average-mass>
