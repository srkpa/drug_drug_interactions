  <name>Butoconazole</name>
  <description>Butoconazole is an imidazole antifungal used in gynecology.</description>
  <cas-number>64872-76-0</cas-number>
  <unii>0Q771797PH</unii>
  <average-mass>411.776</average-mass>
  <monoisotopic-mass>410.017802365</monoisotopic-mass>
  <state>solid</state>
  <groups>
    <group>approved</group>
