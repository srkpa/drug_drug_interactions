  <drugbank-id primary="true">DB06098</drugbank-id>
  <name>PRLX 93936</name>
  <description>PRLX 93936 is selectively toxic to cancer cells.</description>
  <cas-number/>
  <unii/>
  <state>solid</state>
  <groups>
    <group>investigational</group>
  </groups>
  <general-references>
