{
  "dataset": [
    {
      "name": "drugbank",
      "smi_transf": "fgp",
      "dt_transf": "common"
    }
  ],
  "arch": [
    {
      "net": "druud",
      "params": {
        "drug_feature_extractor": {
          "net": "fcfeat",
          "params": {
             "input_size": 2000,
             "hidden_sizes":[100],
              "activation": "ReLU",
              "normalize_features":false,
              "dropout":0.0
          }
           },
          "fc_layers_dim": [1000]
      },
      "init_fn": null
    }
  ],
  "loss_function": [
    "bce"
  ],
  "optimizer": [
    {
      "optim": "adam",
      "params": {
        "lr": 0.0001,
        "weight_decay": 0
      }
    }
  ],
  "train_params": [
    {
      "n_epochs": 1,
      "batch_size": 32,
      "early_stopping": true,
      "reduce_lr": true
    }
  ]
}

